generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  // The 'url' property is deprecated in Prisma 5. Move your connection URL to prisma.config.ts.
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  createdAt DateTime @default(now())
}
